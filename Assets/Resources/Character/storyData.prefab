        isTransition: 0
        background: {fileID: 0}
        hasSoundEffect: 0
        soundEffect: {fileID: 0}
      - bgText: 0
        characterIndex: 0
        dialogText: "\u201CTunggu dulu, ini dimana?!\u201D"
