        isTransition: 0
        background: {fileID: 0}
        hasSoundEffect: 0
        soundEffect: {fileID: 0}
      - bgText: 0
        characterIndex: 0
        dialogText: "\u201CDan dari mana kau tau aku bukan berasal dari sini?!\u201D"
        isQuestion: 0
        isTransition: 0
        background: {fileID: 0}
        hasSoundEffect: 0
        soundEffect: {fileID: 0}
      - bgText: 0
